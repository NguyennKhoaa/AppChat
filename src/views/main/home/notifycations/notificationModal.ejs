<div class="modal fade" id="notifycationModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Tất cả thông báo.</h4>
            </div>
            <div class="modal-body">
                <a class="mark-notif-as-read" href="javascript:void(0)" id="modal-mark-notif-as-read">
                    <strong>Đánh dấu tất cả đã đọc</strong>
                </a>
            </div>
            <hr>
            <ul class="list-notifycations">
                <% notifycations.forEach(function(notifycation){ %>
                <li>
                    <%- notifycation %>
                </li>
                <% });%>
            </ul>
            <hr>
            <div class="read-more-notif">
                <a class="mark-notif-as-read" href="javascript:void(0)" id="link-read-more-notif">
                    <strong>Xem thêm</strong>
                </a>
            </div>
        </div>
    </div>
</div>